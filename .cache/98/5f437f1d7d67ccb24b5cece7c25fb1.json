({error: err}));\r\n    }\r\n\r\n    toggleModal = () => this.setState({showModal: !this.state.showModal});\r\n    \r\n    adopt = () => window.open(this.state.url);\r\n    \r\n    wrapperFunction = () => {\r\n        this.adopt();\r\n        this.toggleModal();\r\n    }\r\n        \r\n    \r\n    render() {\r\n        if (this.state.loading){\r\n            return <h1>loading...</h1>;\r\n        }\r\n\r\n        const {animal, breed, location, description, media, name, url, showModal} = this.state;\r\n\r\n        return (\r\n            <div className=\"details\">\r\n                <Carousel media={media} />\r\n                <div>\r\n                    <h1>{name}</h1>\r\n                    <h2>{`${animal} - ${breed} - ${location}`}</h2>\r\n                    <ThemeContext.Consumer>\r\n                        {([theme]) => (\r\n                            <button style={{ backgroundColor: theme}} onClick={this.toggleModal}>\r\n                                Adopt {name}\r\n                            </button>\r\n                        )}\r\n                    </ThemeContext.Consumer>\r\n                    <p>{description}</p>\r\n                    {\r\n                        showModal ? (\r\n                            <Modal>\r\n                                <div>\r\n                                    <h1>Would you like to adopt {name}</h1>\r\n                                    <div className=\"buttons\">\r\n                                        <button onClick={this.wrapperFunction}>Yes</button>\r\n                                        <button onClick={this.toggleModal}>No</button>\r\n                                    </div>\r\n                                </div>\r\n                            </Modal>\r\n                        ) : null\r\n                    }\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default function DetailsErrorBoundary(props) {\r\n    return (\r\n        <ErrorBoundary>\r\n            <Details {...props} />\r\n        </ErrorBoundary>\r\n    );\r\n}"},"lineCount":null}},"error":null,"hash":"78bbeb7d1153ed477b04d8d1a5385898","cacheData":{"env":{}}}